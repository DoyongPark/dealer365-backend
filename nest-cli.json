{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-crm/src",
  "root": "apps/api-crm",
  "monorepo": true,
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-crm/tsconfig.app.json"
  },
  "projects": {
    "api-crm": {
      "type": "application",
      "root": "apps/api-crm",
      "entryFile": "main",
      "sourceRoot": "apps/api-crm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-crm/tsconfig.app.json"
      }
    },
    "api-sale": {
      "type": "application",
      "root": "apps/api-sale",
      "entryFile": "main",
      "sourceRoot": "apps/api-sale/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-sale/tsconfig.app.json"
      }
    },
    "api-product": {
      "type": "application",
      "root": "apps/api-product",
      "entryFile": "main",
      "sourceRoot": "apps/api-product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-product/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "nest": {
      "type": "library",
      "root": "libs/nest",
      "entryFile": "index",
      "sourceRoot": "libs/nest/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nest/tsconfig.lib.json"
      }
    },
    "crm-service": {
      "type": "library",
      "root": "libs/crm-service",
      "entryFile": "index",
      "sourceRoot": "libs/crm-service/src",
      "compilerOptions": {
        "tsConfigPath": "libs/crm-service/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "crm-database": {
      "type": "library",
      "root": "libs/crm-database",
      "entryFile": "index",
      "sourceRoot": "libs/crm-database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/crm-database/tsconfig.lib.json"
      }
    }
  }
}